<div class="form_container">
<h1 class="ad_field">Admin login</h1>


<%=form_with url: "/admin/login",method: :post do |form| %>
  
    <div class="ad_field">
    <%= form.label :user_name%>
    <%= form.text_field :username ,:class=>"form_box" %>
    </div>

    <div class="ad_field">
    <%= form.label :password %>
    <%= form.password_field :password ,:class=>"form_box"  %>
    </div>

      <div class="ad_field">
    <%= form.submit "  Login  " ,:class=>"form_box" %>
    </div>


   <%end%>

   <div/>
   <footer >
<p>Footer</p>
</footer>